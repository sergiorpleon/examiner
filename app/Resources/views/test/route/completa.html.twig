<div id="completa" class="row form-panel box box-primary" v-show="visibilidad==6">
    <div class="col-sm-12 container">
        <h4>Question Complete</h4>
        <div class="col-xs-12 col-editor">
            <text-ckeditor nnumber="150" :nhab="habilidad" nname="questionc" :ntext="questionC.textoPregunta" @update-text="updateTextComplete" ></text-ckeditor>
        </div>
        <p class="col-sm-12">
            <br>
        </p>
    </div>


    <div class="col-sm-12 container">
        <div class="col-xs-12 col-editor">
            <text-ckeditor nnumber="100" :nhab="habilidad" nname="itemc" :ntext="questionC.completa.vistaHtml" @update-text="updateData" ></text-ckeditor>
        </div>
        <br>

        <br>
        <p>See Previous  - Detected {{"{{questionC.completa.itemsComplete.length}}"}} word. </p>
        <ul>
            <li v-for="(item, index) in questionC.completa.itemsComplete" >
                index in for->{{"{{index}}"}}| orden->{{"{{ item.orden}}"}}| word->{{"{{ item.texto}}"}}
            </li>
        </ul>



        <div v-html="previewPregunta()"></div>
        <p>
            <br>
        </p>
    </div>


    <div class="col-sm-12">

        <button class="btn btn-info" style="margin-top: 20px;" v-on:click="guardarQuestion(questionC)"><i class="fa fa-save"></i> Save</button>
        <button class="btn btn-info" style="margin-top: 20px;" v-on:click="continuarQuestion(questionC)"><i class="fa fa-save"></i> Save and Continue</button>
        <button class="btn btn-warning" style="margin-top: 20px;" v-on:click="cancelarQuestion(questionC)"><i class="fa fa-save"></i> Cancel</button>

        <button class="btn btn-info" style="margin-top: 20px;" v-on:click="showComplete()"><i class="fa fa-eye"></i> Show</button>

    </div>
</div>